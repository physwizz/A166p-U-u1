/* TOUCH start */
&i2c6 {
	#address-cells = <1>;
	#size-cells = <0>;
	clock-frequency = <400000>;
	status = "okay";

	focaltech@38 {
		compatible = "focaltech,fts";
		reg = <0x38>;
		status = "okay";
		interrupt-parent = <&pio>;
		interrupts = <4 0x2002>;
		vdd-supply = <&mt6377_vtp>;
		focaltech,irq-gpio = <&pio 4 0x0>;
		focaltech,display-coords = <0 0 1079 2339>;
		focaltech,max-touch-number = <10>;

	};

};
&i2c6 {
	#address-cells = <1>;
	#size-cells = <0>;
	clock-frequency = <400000>;
	status = "okay";

	tsp_zxt: touchscreen@20 {
		status = "okay";
		compatible = "zinitix,zt_ts_device";
		reg = <0x20>;
		zinitix,regulator_dvdd = "vtp";
		vtp-supply = <&mt6377_vtp>;
		zinitix,irq_gpio = <&pio 4 0x0>;
		zinitix,max_coords = <1080 2340>;
		zinitix,x_resolution = <1080>;
		zinitix,y_resolution = <2340>;
//		zinitix,area-size = <>;
		zinitix,page_size = <128>;

		//feature
		zinitix,mis_cal_check;
		enable_settings_aot;
		support_ear_detect_mode;
		support_dex_mode;
		support_open_short_test;

		zinitix,enable_sysinput_enabled;
		not_support_io_ldo;
	};

};

&tsp_zxt {
		zinitix,max_coords = <1080 2340>;
		zinitix,bitmask_unload = <0x00FF00 0x004200>;

		//calibration
		zinitix,tclm_level = <2>;
		zinitix,afe_base = <0x0006>;

		//This part is for firmware
		zinitix,chip_name = "ZT7650M";
		zinitix,firmware_name = "zt7650m_a16.bin";
		zinitix,bringup = <0>;
};

/* TOUCH end */

